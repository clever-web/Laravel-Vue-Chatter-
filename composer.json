{
    "name": "chatter-laravel/core",
    "description": "A simple forum package for your laravel app",
    "license": "MIT",
    "authors": [
        {
            "name": "Santiago Barbat",
            "email": "barbatsan@gmail.com"
        }
    ],
    "require": {
        "illuminate/cache": "^5.1",
        "illuminate/console": "^5.1",
        "laravel/passport": "^7",
        "mews/purifier": "^3.1",
        "cviebrock/eloquent-sluggable": "^4",
        "laravel-frontend-presets/tailwindcss": "^2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "^3.8",
        "orchestra/testbench-dusk": "^3.8",
        "behat/behat": "^3.5",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.3",
        "laracasts/behat-laravel-extension": "^1.1"
    },
    "autoload": {
	    "psr-4": {
	        "Chatter\\Core\\": "src/"
		}
	},
    "autoload-dev": {
        "psr-4": {
            "Chatter\\Core\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chatter\\Core\\ChatterServiceProvider"
            ]
        }
    }
}
